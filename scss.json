	///////////////////////////
	// Mixins
	///////////////////////////
	
	"staggered-transitions mixin": {
		"prefix": "stag",
		"body": [
			"@include staggered-transitions(${1:1}, ${2:3}, ${3:0}, ${4:200});"
		],
		"description": "staggered-transitions() (custom)"
	},
	
	"font mixin": {
		"prefix": "fontmix",
		"body": [
			"@include font(${1:32}, ${2:48}, ${3:400}, ${4:0});"
		],
		"description": "font() (custom)"
	},
	
	"default background mixin": {
		"prefix": "bgmix",
		"body": [
			"@include default-bg(${1:center}, ${2:cover}, ${3:no-repeat});"
		],
		"description": "default-bg() (custom)"
	},
	
	
	
	
	
	
	
	
	///////////////////////////
	// rem-calc()
	///////////////////////////
	
	
	"rem-calc()": {
	 "prefix": "remc",
	 "body": [
		 "rem-calc($1)"
	 ],
	 "description": "rem-calc() (custom)"
	},
	
	"font-size: rem-calc()": {
